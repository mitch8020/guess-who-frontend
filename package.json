{
  "name": "guess-who-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vite build && tsc",
    "start": "serve -s dist -l $PORT",
    "heroku-postbuild": "npm run build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov:unit": "vitest run --config vitest.layer.unit.config.ts",
    "test:cov:integration": "vitest run --config vitest.layer.integration.config.ts",
    "test:cov:component": "vitest run --config vitest.layer.component.config.ts",
    "test:cov:e2e": "vitest run --config vitest.layer.e2e.config.ts",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix"
  },
  "dependencies": {
    "@rollbar/react": "^0.12.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollbar": "^2.26.5",
    "serve": "^14.2.4",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.0.6"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
