{
  "name": "guess-who-frontend",
  "description": "Guess Who frontend web app (Vite/React)",
  "env": {
    "VITE_API_BASE_URL": {
      "description": "Backend base URL, e.g. https://<backend-app>.herokuapp.com/api"
    },
    "VITE_WS_BASE_URL": {
      "description": "Backend websocket URL, e.g. wss://<backend-app>.herokuapp.com/ws"
    },
    "VITE_ROLLBAR_CLIENT_TOKEN": {
      "required": false
    },
    "VITE_ROLLBAR_ENV": {
      "value": "production"
    },
    "VITE_ROLLBAR_CODE_VERSION": {
      "value": "heroku-release"
    },
    "VITE_ENABLE_TANSTACK_DEVTOOLS": {
      "value": "false"
    }
  }
}
